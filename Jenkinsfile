pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo "This is a build stage"
            }
        }
        stage('build new'){
            steps{
                echo "This is a  New build stage"
            }
        }
        stage('build Test'){
            steps{
                echo "This is a  New build Test"
            }
        }

    }
}
